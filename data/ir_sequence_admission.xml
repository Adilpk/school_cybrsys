<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
<!--  Admission number sequence  -->
    <record id="admission_number_id" model="ir.sequence">
        <field name="name">My Sequence</field>
        <field name="code">my_sequence_code</field>
        <field name="prefix">ADSCHOOL</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
    </record>
 <!-- Register Number Sequence -->
    <record id="register_sequence_id" model="ir.sequence">
            <field name="name">My register Sequence</field>
            <field name="code">register_sequence_code</field>
            <field name="prefix">REG</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
    </record>
</odoo>